<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<table class="table">
  <thead>
  <tr>
    <th scope="col">Avatar</th>
    <th scope="col">Full Name</th>
    <th scope="col">Email</th>
    <th scope="col"></th>
    <th scope="col"></th>
  </tr>
  </thead>
  <tbody class="users">
  <% @users.each do |user| %>
    <%= render user %>
  <% end %>
  </tbody>
</table>

<%= will_paginate %>